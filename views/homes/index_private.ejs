    <link rel='stylesheet' href='/stylesheets/style.css' />

    <script>
      sessionStorage.clear();
      sessionStorage.siteName = "<%= siteName %>";
      sessionStorage.logo = "<%= logo %>";
      sessionStorage.logoWhite = "<%= logoWhite %>";
    </script>
  </head>
  <body>
    <h1 class="title"></h1>
    <img class="site-logo" src="">

    <div class="desc">A Private Company</div>
